<div class="container">
  <div class="middle_grid wow fadeInUp" data-wow-delay="0.4s">
    <% @sailings.where('id < 11').find_each() do |sailing| %>
        <!-- LEFT SECTION OF PANEL -->
        <div class="col-md-6 index-grids">
          <div class="grid1">
            <div class="view view-first">
              <div class="index_img1"><%= image_tag "pic4.jpg" %></div>
              <div class="mask">
                <h2 style="text-align: center">Cruise Info<br/><%= sailing.cruise_ship_name %></h2>
                <div class="inner_wrap1">
                  <h4>Registered Events</h4>
                  <div class="scrolling">
                    <table class="table">
                      <thead>
                      <tr>
                        <th>Event</th>
                        <th>Description</th>
                        <th>Participants</th>
                      </tr>
                      </thead>
                      <tbody>

                      <% sailing.events.each do |event| %>
                          <tr>
                            <td><%= event.event_name %></td>
                            <td><%= event.description %></td>
                            <td><%= event.max_participants %></td>
                          </tr>
                      <% end %>

                      </tbody>
                    </table>
                  </div>
                  <label class="btn1 btn-3 btn-2g">
                    <%= link_to 'Full Details', sailing %>
                  </label>
                </div>
                <div class="clearfix"></div>
                </ul>
              </div>
            </div>
            <div class="inner_wrap">
              <ul class="star1">
                <li class="module_left">
                  <h3><%= sailing.cruise_ship_company %></h3>
                  <h2><%= sailing.cruise_ship_name %></h2>
                  <h4>No. of Cruisers: 1,899</h4>
                  <h4>Region: <%= sailing.destination_identifier %> </h4>
                  <h4>Port of Origin: <%= sailing.port_of_origin %> </h4>
                  <h4>Departure: <%= sailing.departure_date.strftime("%B %d, %Y") %></h4>
                  <h4>Return: <%= sailing.return_date.strftime("%B %d, %Y") %></h4>
                </li>
              </ul>
            </div>
          </div>
        </div>
    <% end %>
  </div>
